{
  "compilerOptions": {
    "types": [
      "babel__core",
      "babel__generator",
      "babel__template",
      "babel__traverse",
      "estree",
      "prop-types",
      "react",
      "react-dom"
    ],
    "typeRoots": ["./node_modules/@types"],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "vite.config.ts"
  ]
}